// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+alt+v",
        "command": "toggleVim"
    },
    {
        "key": "shift shift",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "f9",
        "command": "notebook.execute",
        "when": "!notebookHasSomethingRunning || config.notebook.globalToolbar && notebookInterruptibleKernel || !notebookInterruptibleKernel && notebookKernelCount > 0 || !notebookInterruptibleKernel && notebookKernelSourceCount > 0"
    },
    {
        "key": "f9",
        "command": "-editor.debug.action.toggleBreakpoint",
        "when": "debuggersAvailable && disassemblyViewFocus || debuggersAvailable && editorTextFocus"
    }
]
